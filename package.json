{
  "name": "chess-tournament-director",
  "version": "1.0.0",
  "description": "A comprehensive chess tournament director pairing system",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "setup": "node server/scripts/create-admin.js",
    "start": "node server/index.js",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "@types/jspdf": "^1.3.3",
    "archiver": "^7.0.1",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dbffile": "^1.12.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.2",
    "selenium-webdriver": "^4.15.0",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "validator": "^13.11.0"
  },
  "keywords": [
    "chess",
    "tournament",
    "pairing",
    "swiss-system",
    "uscf"
  ],
  "author": "Chess Tournament Director",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
